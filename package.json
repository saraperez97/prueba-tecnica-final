{
  "scripts": {
    "install:all": "cd backend-users && npm install && cd ../frontend-users && npm install && cd .. && npm install",
    "start:backend": "cd backend-users && node server.js",
    "start:frontend": "cd frontend-users && ng serve --open",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
